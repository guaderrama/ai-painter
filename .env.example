# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Your Supabase URL (from your project dashboard)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here

# Your Supabase anon key (from your project dashboard)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Your Supabase service role key (from your project dashboard)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Supabase Project Reference ID (from your project dashboard)
SUPABASE_PROJECT_REF=your_project_ref_here
